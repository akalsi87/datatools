# datatoolsConfig.cmake

# - Config file for the datatools package
# It defines the following variables
#  DATATOOLS_INCLUDE_DIRS - include directories for datatools
#  DATATOOLS_LIBRARIES    - libraries to link against
#  DATATOOLS_EXECUTABLE   - executable if any

# Compute paths
get_filename_component(DATATOOLS_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(DATATOOLS_INCLUDE_DIRS include)

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET datatools AND NOT DATATOOLS_BINARY_DIR)
  include("${DATATOOLS_CMAKE_DIR}/datatoolsTargets.cmake")
endif()

# These are IMPORTED targets created by datatoolsTargets.cmake
set(DATATOOLS_LIBRARIES datatools)
set(DATATOOLS_EXECUTABLE )
